{
  "name": "rheostat",
  "version": "2.0.0",
  "description": "Rheostat is a www, mobile, and accessible slider component built with React",
  "main": "lib/Slider.js",
  "jsnext:main": "src/Slider.jsx",
  "scripts": {
    "build": "npm run clean && babel src -d lib",
    "clean": "rimraf lib",
    "prepublish": "npm run build",
    "lint": "eslint src/Slider.jsx src/utils/*.js",
    "storybook": "start-storybook -p 9001",
    "test": "npm run build && npm run lint && npm run test:coverage",
    "test:coverage": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- -R tap 'test/**/*-test.js'",
    "test:quick": "babel-node node_modules/.bin/_mocha -R tap test/*-test.js",
    "watch": "npm run clean && babel src --watch -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:airbnb/rheostat.git"
  },
  "author": "Josh Perez <josh.perez@airbnb.com>",
  "license": "MIT",
  "dependencies": {
    "color": "^0.11.3",
    "global-cache": "^1.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.26.0",
    "aphrodite": "^0.6.0",
    "babel-cli": "^6.9.0",
    "babel-istanbul": "^0.8.0",
    "babel-preset-airbnb": "^1.0.1",
    "casual": "^1.5.3",
    "chai": "^3.4.1",
    "enzyme": "^1.3.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.7.1",
    "mocha": "^2.3.3",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.3",
    "react-with-styles": "^1.0.0",
    "react-with-styles-interface-aphrodite": "^1.0.0",
    "rimraf": "^2.3.2",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.1"
  },
  "peerDependencies": {
    "react": ">=0.12.x"
  }
}
